    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "markdown-it": "^13.0.1"
  },
  "bugs": {
    "url": "https://github.com/Jaspreet-singh-1032/DSA-450/issues"
  },
  "repository": {

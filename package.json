  "description": "An offline vscode extension to practice a list of topic-wise DSA questions.",
  "version": "0.0.1",
  "publisher": "JaspreetSingh",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other",
    "Education"
  ],
  "activationEvents": [
    "onCommand:DSA450.showTopicsList"
